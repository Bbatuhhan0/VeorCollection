    /* 3. Sidebar (Sol Menü) Stilleri */
    aside.sidebar .sidebar-widget {
        background-color: #0a0a0a !important;
        border: 1px solid #333 !important;
    }

    .sidebar-title-two h5 {
        color: #dabd82 !important; /* Başlıklar Gold */
        border-bottom: 1px solid #333;
    }

    /* Kategori Listesi */
    .category-list li a {
        color: #cccccc !important;
    }

        .category-list li a:hover {
            color: #dabd82 !important;
        }

        .category-list li a span {
            color: #666 !important;
        }

    /* Markalar (Checkboxlar) */
    .brands-list .check-box label {
        color: #cccccc !important;
    }

    .brands-list .check-box input[type="checkbox"] + label:before {
        border-color: #666 !important;
    }

    .brands-list .check-box input[type="checkbox"]:checked + label:before {
        background-color: #dabd82 !important;
        border-color: #dabd82 !important;
    }

    /* Etiketler (Tags) */
    .tag-list li a {
        background-color: #1a1a1a !important;
        color: #ccc !important;
        border: 1px solid #333;
    }

        .tag-list li a:hover {
            background-color: #dabd82 !important;
            color: #000 !important;
            border-color: #dabd82 !important;
        }

    /* 4. İçerik Alanı (Sağ Taraf) */
    .content h3 {
        color: #ffffff !important;
    }

    .content p {
        color: #cccccc !important;
    }

    /* Filtre Kutusu */
    .filter-box {
        background-color: #0a0a0a !important;
        border: 1px solid #333 !important;
    }

        .filter-box .results {
            color: #dabd82 !important;
        }

        .filter-box select {
            background-color: #000 !important;
            color: #fff !important;
            border: 1px solid #333;
        }

    .pages-list li a {
        color: #fff !important;
    }

        .pages-list li a:hover {
            color: #dabd82 !important;
        }

    /* 5. Ürün Kartları (Shop Item) */
    .shop-item .inner-box {
        background-color: #0a0a0a !important;
        border: 1px solid #333 !important;
        transition: all 0.3s ease;
    }

        .shop-item .inner-box:hover {
            border-color: #dabd82 !important;
        }

    .shop-item .lower-content h6 a {
        color: #ffffff !important;
    }

        .shop-item .lower-content h6 a:hover {
            color: #dabd82 !important;
        }

    /* Fiyat */
    .shop-item .price {
        color: #dabd82 !important;
        font-weight: bold;
    }

        .shop-item .price span {
            color: #666 !important;
            text-decoration: line-through;
        }

    /* Rating Yıldızlar */
    .rating .fa {
        color: #dabd82 !important;
    }

    .rating .light {
        color: #444 !important;
    }

    /* Miktar Kutusu */
    .item-quantity input.qty-spinner {
        background-color: #000 !important;
        border: 1px solid #333 !important;
        color: #fff !important;
    }

    /* 6. Pagination */
    .styled-pagination li a {
        color: #ffffff !important;
        background-color: transparent !important;
        border: 1px solid #333 !important;
    }

    .styled-pagination li.next a,
    .styled-pagination li.prev a {
        color: #ffffff !important;
        background-color: #000 !important;
    }

    .styled-pagination li a:hover,
    .styled-pagination li a.active {
        background-color: #dabd82 !important;
        color: #000000 !important;
        border-color: #dabd82 !important;
    }

    /* 7. Sidebar Cart & Offcanvas Fix */
    .offcanvas, .sidebar-widget-container {
        background-color: #000000 !important;
        color: #fff !important;
    }

        .sidebar-widget-container h6 {
            color: #dabd82 !important;
        }

        .sidebar-widget-container a {
            color: #ccc !important;
        }

