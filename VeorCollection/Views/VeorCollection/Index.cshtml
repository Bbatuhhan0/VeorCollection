	/* anasayfa profil yüksekliği */

	.main-slider-carousel .slide {
		height: 700px;
	}

		.main-slider-carousel .slide .image-layer {
			height: 100%;
		}

	/* 1. Sayfa Arka Planını Siyah Yapma */
	body {
		background-color: #000000 !important; /* Sayfanın arka planını siyah yapar */
		color: #ffffff; /* Sayfadaki varsayılan metin rengini beyaz yapar */
	}

	/* 4. AÇILIR MENÜ (DROPDOWN) DÜZELTMESİ */

	/* Açılır menünün arka planını siyah yapar */
	.main-menu .navigation li.dropdown ul {
		background-color: #000000 !important;
		/* Temanızın varsayılan kenarlığını kaldırabilir veya
						uygun bir renkle değiştirebilirsiniz: */
		border: 1px solid #333; /* Çok koyu gri bir kenarlık */
	}

		/* Açılır menüdeki linklerin (li > a) rengini beyaz yapar */
		.main-menu .navigation li.dropdown ul li a {
			color: #ffffff !important;
			/* Varsa, varsayılan hover arka planını kaldırır */
			background-color: transparent !important;
		}

			/* Açılır menüdeki linklerin (li > a) hover rengini ayarlar */
			.main-menu .navigation li.dropdown ul li a:hover {
				color: #dabd82 !important;
				/* Hover'da da arka planın değişmediğinden emin olur */
				background-color: transparent !important;
			}

	/* 5. FEATURED VE BRAND BÖLÜMLERİ ARASINA BOŞLUK EKLEME */
	.featured-section {
		margin-top: 30px; /* Üstteki slider'dan ayırmak için üst boşluk eklendi */
		/* İhtiyacınıza göre bu değeri artırıp azaltabilirsiniz */
		margin-bottom: 31px;
	}

	/* 3. Siyah Arka Plan İçin Ek Düzeltmeler (Yazıların Görünmesi) */

	/* Ana bölüm başlıkları (Örn: "Products your choich !") */
	.sec-title h2 {
		color: #ffffff !important;
	}

	/* "Featured" bölümü (Free Shipping, Payment Secure vb.) */
	.feature-block .content h2 {
		color: #ffffff;
	}

	.feature-block .content .text {
		color: #dddddd; /* Ana metinden biraz daha soluk */
	}

	/* "Sale" bölümü yazıları */
	.sale-block .overlay-inner h5 a {
		color: #ffffff;
	}

		.sale-block .overlay-inner h5 a:hover {
			color: #dabd82;
		}

	.sale-block .overlay-inner .off {
		color: #ffffff;
	}

	/* "Counter" bölümü (5k+ Fashion Product...) */
	.counter-block .inner-box h2 {
		color: #ffffff;
	}

	/* "Products" bölümü filtreleri */
	.products-section-three .filters .filter-tabs li {
		color: #ffffff;
	}

		.products-section-three .filters .filter-tabs li:hover,
		.products-section-three .filters .filter-tabs li.active {
			color: #dabd82; /* Aktif ve hover rengi */
		}

	/* Ürün kartı metinleri */
	.shop-item .lower-content h6 a {
		color: #ffffff;
	}

		.shop-item .lower-content h6 a:hover {
			color: #dabd82;
		}

	.shop-item .lower-content .price {
		color: #dddddd;
	}

	/* "Testimonial" (Müşteri Yorumları) bölümü */
	.testimonial-block .inner-box .text {
		color: #000000 !important; /* Kullanıcı isteği üzerine siyaha çevrildi */
	}

	/* 6. BUTON STİLİ (Shop Now) - (Ters Çevrilmiş Efekt) */
	.theme-btn.btn-style-one {
		background-color: #ffffff !important; /* Arka plan beyaz */
		color: #000000 !important; /* Yazı siyah */
		border-color: #ffffff !important; /* Kenarlık beyaz */
		transition: all 0.3s ease; /* Yumuşak geçiş efekti */
	}

		/* Butonun üzerine gelindiğinde (hover) */
		.theme-btn.btn-style-one:hover {
			background-color: #dabd82 !important; /* Arka plan rengi */
			color: #000000 !important; /* Yazı rengi (siyah daha iyi okunur) */
			border-color: #dabd82 !important; /* Kenarlık rengi */
		}

	/*2 tablo yükseklik*/

	.brand-section {
		margin-bottom: -70px; /* -32px idi, sale-section'ı yukarı çekmek için artırıldı */
	}

	/* 7. YORUM VE SAYFALAMA BÖLÜMÜ DÜZENLEMESİ */
	/* Genel sayfalama elemanları için renkleri ayarlar */
	.styled-pagination li a,
	.page-pagination li a {
		color: #ffffff !important; /* Sayfa numaraları ve oklar beyaz olsun */
		background-color: transparent !important; /* Arka plan şeffaf olsun */
		border-color: #555 !important; /* Hafif bir kenarlık ekler */
	}

		/* Aktif sayfa ve hover efekti */
		.styled-pagination li.active a,
		.styled-pagination li a:hover,
		.page-pagination li.active a,
		.page-pagination li a:hover {
			color: #dabd82 !important; /* Aktif ve üzerine gelinen sayfa numaraları bu renkte olsun */
			border-color: #dabd82 !important; /* Aktif ve üzerine gelinenin kenarlığı bu renkte olsun */
			/* İsterseniz aktif veya hover'a arka plan rengi verebilirsiniz:
			background-color: rgba(218, 189, 130, 0.1) !important;
			*/
		}

			/* Aktif sayfanın altındaki çizgi */
			.styled-pagination li.active a:after,
			.page-pagination li.active a:after {
				background-color: #dabd82 !important; /* Çizginin rengi */
			}

	/* 8. BRAND SECTION ARKA PLAN VE YAZI RENGİ (Düzeltildi) */
	/* 'brand-section' içindeki 'outer-container'ın arka planını siyah yapar */
	.brand-section .outer-container {
		background-color: #000000 !important;
	}

	/* .brand-section içindeki 'Design. Brand. Quality' metninin rengi */
	.brand-section .animation_mode h1 {
		color: #ffffff !important; /* Yazı rengini beyaz yapar */
	}

	.sale-section {
		margin-top: 10px; /* Yazı rengini beyaz yapar */
	}

	/* ... (Mevcut 8 kuralınız burada) ... */
	/* 9. TESTIMONIAL (YORUM) OK-SAYFALAMA ÇAKIŞMASI DÜZELTMESİ */
	.testimonial-section .owl-nav {
		/* Okları (01, 02, 03) yazan mavi çubuğun üstüne gelmeyecek şekilde
			   biraz yukarı kaydıralım. */
		transform: translateY(-40px); /* Okları 40px yukarı taşır. */
		/* Eğer -40px yetmez veya fazla gelirse, bu değeri artırıp (örn: -50px)
			   azaltarak (örn: -30px) tam istediğiniz yere getirebilirsiniz. */
	}

	/* ... (Mevcut 9 kuralınız burada) ... */
	/* 10. TESTIMONIAL (YORUM) SAYFALAMA NOKTALARINI GİZLEME */
	.testimonial-section .owl-dots {
		display: none !important; /* Sayfalama noktalarını tamamen gizler */
	}

	/* Okların yukarı kaydırılmasına artık gerek kalmayabilir,
		   çünkü alttaki çubuk gitti. Önceki transform değerini sıfırlayalım
		   veya ayarlayalım. */
	.testimonial-section .owl-nav {
		transform: translateY(0) !important; /* Okları orijinal dikey konumuna geri getirir */
		top: 50% !important; /* Yorum bloğunun dikey ortasına hizalar */
		margin-top: -15px !important; /* Ok ikonlarının tam ortalanması için ufak ayar */
	}

		/* 11. TESTIMONIAL (YORUM) OKLARINI BİRBİRİNE YAKLAŞTIRMA */
		.testimonial-section .owl-nav .owl-prev {
			/* Sol oku soldan ayırıp merkeze doğru iter. */
			/* 'auto' yaparak sağdaki okla hizalanmasını kolaylaştırabiliriz */
			left: auto !important;
			right: -60px !important; /* Sağ okun 60px soluna yerleşecek */
		}

		.testimonial-section .owl-nav .owl-next {
			/* Sağ oku sağ kenardan biraz ayırır. */
			right: 10px !important;
		}

	/* 12. TESTIMONIAL (YORUM) BÖLÜMÜ YÜKSEKLİĞİNİ AZALTMA */
	.testimonial-section {
		/* Muhtemelen temanızda 100px veya 120px gibi yüksek değerler var.
			   Bunları ezip daha makul bir seviyeye çekelim. */
		padding-top: 45px !important;
		padding-bottom: 43px !important;
	}

